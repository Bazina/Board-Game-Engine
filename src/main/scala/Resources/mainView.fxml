<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainController">
   <children>
      <AnchorPane fx:id="gamePane" layoutY="48.0" prefHeight="672.0" prefWidth="1080.0" />
      <AnchorPane fx:id="menuPane" prefHeight="720.0" prefWidth="1080.0">
         <children>
            <Button layoutX="382.0" layoutY="328.0" mnemonicParsing="false" prefHeight="84.0" prefWidth="379.0" text="Connect-4">
               <font>
                  <Font size="50.0" />
               </font>
            </Button>
            <Button layoutX="382.0" layoutY="221.0" mnemonicParsing="false" prefHeight="107.0" prefWidth="379.0" text="Checkers">
               <font>
                  <Font size="50.0" />
               </font>
            </Button>
            <Button layoutX="382.0" layoutY="114.0" mnemonicParsing="false" prefHeight="107.0" prefWidth="379.0" text="Chess">
               <font>
                  <Font size="50.0" />
               </font>
            </Button>
            <Button layoutX="382.0" layoutY="7.0" mnemonicParsing="false" prefHeight="107.0" prefWidth="379.0" text="X-O">
               <font>
                  <Font size="50.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="switchPane" prefHeight="54.0" prefWidth="1080.0">
         <children>
            <ImageView fitHeight="54.0" fitWidth="63.0" onMouseClicked="#switchPanes" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@previous.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
